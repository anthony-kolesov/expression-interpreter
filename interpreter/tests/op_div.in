# Typical operation.
out 6 / 3
print "\n"

# Relatively big numbers.
out 1000000 / 2
print "\n"

# Crossing the zero.
out 36 / -9
print "\n"

# Staying negative.
out -21 / 7 
print "\n"

# Floating point numbers.
out 3.9 / 1.3
print "\n"

# Floating point and integer numbers.
out 8 / 2.5
print "\n"

# Zero values.
out 0 / 1
print "\n"

# Division by one.
out 7 / 1
print "\n"

# That will be 0, because result is integer.
out 1 / 3
print "\n"

# That will be 0.5, because result is float.
out 1.0 / 2
print "\n"
